{
  "version": 1,
  "projects": {
    "seriea": {
      "root": "apps/seriea",
      "sourceRoot": "apps/seriea/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "differentialLoading": true,
            "outputPath": "dist/apps/seriea",
            "index": "apps/seriea/src/index.html",
            "main": "apps/seriea/src/main.tsx",
            "polyfills": "apps/seriea/src/polyfills.ts",
            "tsConfig": "apps/seriea/tsconfig.app.json",
            "assets": ["apps/seriea/src/favicon.ico", "apps/seriea/src/assets"],
            "styles": ["apps/seriea/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/seriea/src/environments/environment.ts",
                  "with": "apps/seriea/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "seriea:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "seriea:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "tslint",
            "tsConfig": [
              "apps/seriea/tsconfig.app.json",
              "apps/seriea/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/seriea/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/seriea/jest.config.js",
            "tsConfig": "apps/seriea/tsconfig.spec.json"
          }
        }
      }
    },
    "seriea-e2e": {
      "root": "apps/seriea-e2e",
      "sourceRoot": "apps/seriea-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/seriea-e2e/cypress.json",
            "tsConfig": "apps/seriea-e2e/tsconfig.e2e.json",
            "devServerTarget": "seriea:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "seriea:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "tslint",
            "tsConfig": ["apps/seriea-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/seriea-e2e/**"]
          }
        }
      }
    },
    "home": {
      "root": "libs/home",
      "sourceRoot": "libs/home/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "tslint",
            "tsConfig": [
              "libs/home/tsconfig.lib.json",
              "libs/home/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/home/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/home/jest.config.js",
            "tsConfig": "libs/home/tsconfig.spec.json"
          }
        }
      }
    },
    "standings": {
      "root": "libs/standings",
      "sourceRoot": "libs/standings/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "tslint",
            "tsConfig": [
              "libs/standings/tsconfig.lib.json",
              "libs/standings/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/standings/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/standings/jest.config.js",
            "tsConfig": "libs/standings/tsconfig.spec.json"
          }
        }
      }
    },
    "teams": {
      "root": "libs/teams",
      "sourceRoot": "libs/teams/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "tslint",
            "tsConfig": [
              "libs/teams/tsconfig.lib.json",
              "libs/teams/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/teams/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/teams/jest.config.js",
            "tsConfig": "libs/teams/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/react:application": {
      "babel": true
    }
  },
  "defaultProject": "seriea"
}
