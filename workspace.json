{
  "version": 1,
  "projects": {
    "seriea": {
      "root": "apps/seriea",
      "sourceRoot": "apps/seriea/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "differentialLoading": true,
            "outputPath": "dist/apps/seriea",
            "index": "apps/seriea/src/index.html",
            "main": "apps/seriea/src/main.tsx",
            "polyfills": "apps/seriea/src/polyfills.ts",
            "tsConfig": "apps/seriea/tsconfig.app.json",
            "assets": ["apps/seriea/src/favicon.ico", "apps/seriea/src/assets"],
            "styles": ["apps/seriea/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/seriea/src/environments/environment.ts",
                  "with": "apps/seriea/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "seriea:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "seriea:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "tslint",
            "tsConfig": [
              "apps/seriea/tsconfig.app.json",
              "apps/seriea/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/seriea/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/seriea/jest.config.js",
            "tsConfig": "apps/seriea/tsconfig.spec.json"
          }
        }
      }
    },
    "seriea-e2e": {
